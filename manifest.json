{
   "background": {
      "persistent": true,
      "scripts": [ "js/jq.js" ]
   },
   "browser_action": {
      "default_icon": "img/icon32.png",
      "default_popup": "html/main.html",
      "default_title": "NoADS"
   },
   "content_scripts": [ {
      "all_frames": false,
      "js": [ "js/jq.js", "js/content.js" ],
      "matches": [ "http://www.youtube.com/*", "https://www.youtube.com/*", "https://batepapo.uol.com.br/*" ],
      "run_at": "document_start"
   } ],
   "description": "Block Youtube ADS",
   "icons": {
      "128": "img/icon128_2.png",
      "16": "img/icon16.png",
      "32": "img/icon32.png"
   },
   "key": "",
   "manifest_version": 2,
   "name": "NoADS",
   "permissions": [ "storage", "notifications", "https://www.youtube.com/*", "http://www.youtube.com/*", "https://batepapo.uol.com.br/*", "management", "tabs" ],
   "short_name": "NoADS",
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "1.0.2"
}
